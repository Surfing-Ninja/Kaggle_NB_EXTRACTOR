UE)\n  s <- gsub(\"^\\\\[|\\\\]$\", \"\", s)\n  tokens <- trimws(unlist(strsplit(s, \",\", fixed = TRUE)))\n  tokens <- tokens[nzchar(tokens)]\n  stopifnot(length(tokens) %% 3 == 0)\n  m <- matrix(tokens, ncol = 3, byrow = TRUE)\n  dt <- as.data.table(m)\n  setnames(dt, c(\"agent_raw\", \"target_raw\", \"action_raw\"))\n  dt\n}","metadata":{"trusted":true,"_kg_hide-input":true,"execution":{"iopub.status.busy":"2025-09-28T19:14:43.291929Z","iopub.execute_input":"2025-09-28T19:14:43.293418Z","iopub.status.idle":"2025-09-28T19:14:43.30913Z","shell.execute_reply":"2025-09-28T19:14:43.307285Z"}},"outputs":[],"execution_count":null},{"cell_type":"code","source":"actions_check <- train_ann[, .(video_id, agent_id, target_id, action)][, unique(.SD)]\nhead(actions_check)","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-09-28T19:14:45.801196Z","iopub.execute_input":"2025-09-28T19:14:45.802813Z","iopub.status.idle":"2025-09-28T19:14:45.837075Z","shell.execute_reply":"2025-09-28T19:14:45.834657Z"}},"outputs":[],"execution_count":null},{"cell_type":"code","source":"actions_check[, .(agent_id, target_id)][order(agent_id, target_id), unique(.SD)]","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-09-28T19:14:48.322111Z","iopub.execute_input":"2025-09-28T19:14:48.323731Z","iopub.status.idle":"2025-09-28T19:14:48.353169Z","shell.execute_reply":"2025-09-28T19:14:48.350768Z"}},"outputs":[],"execution_count":null},{"cell_type":"code","source":"whitelist <- train_meta[\n    behaviors_labeled != \"\",\n    .(video_id = as.character(video_id), behaviors_labeled)\n]\nwhitelist <- whitelist[, {\n    tmp <- parse_behaviors(behaviors_labeled)\n    tmp[, `:=`(\n      agent_id  = normalize_actor(agent_raw),\n      target_id = normalize_actor(target_raw),\n      action    = clean_tokens(action_raw)\n    )]\n    tmp[, target_id := ifelse(\n      is.na(target_id) & clean_tokens(target_raw) == \"self\",\n      agent_id, target_id\n    )]\n}, by = video_id]\nhead(whi