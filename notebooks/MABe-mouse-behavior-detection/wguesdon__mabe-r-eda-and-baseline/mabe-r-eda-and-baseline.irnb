{"metadata":{"kernelspec":{"name":"ir","display_name":"R","language":"R"},"language_info":{"name":"R","codemirror_mode":"r","pygments_lexer":"r","mimetype":"text/x-r-source","file_extension":".r","version":"4.4.0"},"kaggle":{"accelerator":"none","dataSources":[{"sourceId":59156,"databundleVersionId":13719397,"sourceType":"competition"}],"dockerImageVersionId":30749,"isInternetEnabled":false,"language":"r","sourceType":"notebook","isGpuEnabled":false}},"nbformat_minor":4,"nbformat":4,"cells":[{"cell_type":"code","source":"# MABe Challenge - Social Action Recognition in Mice\n# Enhanced EDA in R with Markdown-style Output and Larger Plots\n# ==================================================\n# Baseline based on:\n# MABe Validated baseline without machine learning\n# https://www.kaggle.com/code/ambrosm/mabe-validated-baseline-without-machine-learning\n# MABe Challenge - BASELINE\n# https://www.kaggle.com/code/snnguynvnk19hl/mabe-challenge-baseline\n# ==================================================\n\n# ==================================================\n# CONFIGURATION OPTIONS\n# ==================================================\nSAVE_EDA_FILES <- TRUE  # Set to FALSE to only generate submission.csv without EDA outputs\nDISPLAY_PLOTS <- TRUE   # Set to TRUE to display plots in notebook\nVERBOSE_OUTPUT <- TRUE  # Set to FALSE for minimal console output\n\n# Load required libraries\nsuppressPackageStartupMessages({\n  library(tidyverse)\n  library(arrow)\n  library(scales)\n  library(viridis)\n  library(gridExtra)\n  library(data.table)\n  library(crayon)  # For colored console output\n})\n\n# Set theme with larger base size for better readability\ntheme_set(theme_minimal(base_size = 16))\ncolor_palette <- viridis(10)\n\n# Set default plot size for all plots\noptions(repr.plot.width = 14, repr.plot.height = 10)\n\n# Helper function to print markdown-style headers\nprint_header <- function(text, level = 1) {\n  separator <- switch(level,\n    \"1\" = paste0(rep(\"=\", 50), collapse = \"\"),\n    \"2\" = paste0(rep(\"-\", 40), collapse = \"\"),\n    \"3\" = paste0(rep(\".\", 30), collapse = \"\")\n  )\n  \n  if(level == 1) {\n    cat(\"\\n\", bold(blue(separator)), \"\\n\", sep = \"\")\n    cat(bold(blue(text)), \"\\n\")\n    cat(bold(blue(separator)), \"\\n\\n\", sep = \"\")\n  } else if(level == 2) {\n    cat(\"\\n\", cyan(separator), \"\\n\", sep = \"\")\n    cat(bold(cyan(text)), \"\\n\")\n    cat(cyan(separator), \"\\n\\n\", sep = \"\")\n  } else {\n    cat(\"\\n\", green(text), \"\\n\", sep = \"\")\n    cat(green(separator), \"\\n\\n\", sep = \"\")\n  }\n}\n\n# Helper function for success messages\nprint_success <- function(text) {\n  cat(green(\"âœ“\"), text, \"\\n\")\n}\n\n# Helper function for warning messages\nprint_warning <- function(text) {\n  cat(yellow(\"âš ï¸\"), yellow(text), \"\\n\")\n}\n\n# Helper function for info messages\nprint_info <- function(text) {\n  cat(blue(\"â„¹\"), text, \"\\n\")\n}\n\n# Helper function for bullet points\nprint_bullet <- function(text, indent = 0) {\n  spaces <- paste0(rep(\" \", indent), collapse = \"\")\n  cat(spaces, \"â€¢ \", text, \"\\n\", sep = \"\")\n}\n\nprint_header(\"MABe CHALLENGE - EXPLORATORY DATA ANALYSIS\", 1)\n\nif(SAVE_EDA_FILES) {\n  print_info(\"ğŸ“Š EDA Mode: ENABLED (will save all analysis files)\")\n} else {\n  print_info(\"âš¡ Quick Mode: ENABLED (will only generate submission.csv)\")\n}\n\n# Create EDA folder if needed\nif(SAVE_EDA_FILES && !dir.exists(\"EDA\")) {\n  dir.create(\"EDA\")\n  print_success(\"Created EDA folder for outputs\")\n}\n\n# ============================================\n# 1. LOAD MAIN CSV FILES\n# ============================================\n\nprint_header(\"STEP 1: LOADING MAIN DATA FILES\", 2)\n\n# Define base path\nbase_path <- \"/kaggle/input/MABe-mouse-behavior-detection\"\n\n# Alternative paths to try if the first doesn't work\nalternative_paths <- c(\n  \"/kaggle/input/mabe-mouse-behavior-detection\",\n  \"/kaggle/input/mabe-challenge-social-action-recognition-in-mice\"\n)\n\n# Find the correct path\ncorrect_path <- NULL\nif(file.exists(file.path(base_path, \"train.csv\"))) {\n  correct_path <- base_path\n  print_success(sprintf(\"Found data at: %s\", correct_path))\n} else {\n  for(path in alternative_paths) {\n    if(file.exists(file.path(path, \"train.csv\"))) {\n      correct_path <- path\n      base_path <- path\n      print_success(sprintf(\"Found data at: %s\", correct_path))\n      break\n    }\n  }\n}\n\nif(is.null(correct_path)) {\n  print_warning(\"Data not found. Trying to list available datasets...\")\n  if(dir.exists(\"/kaggle/input\")) {\n    datasets <- list.dirs(\"/kaggle/input\", recursive = FALSE, full.names = FALSE)\n    cat(\"\\nAvailable datasets:\\n\")\n    for(d in datasets) {\n      print_bullet(d)\n    }\n    cat(\"\\nPlease check the dataset name and update the base_path variable.\\n\")\n  }\n  stop(\"Cannot find the MABe dataset. Please add it to your notebook.\")\n}\n\n# Load train.csv\ntrain <- read_csv(file.path(base_path, \"train.csv\"), show_col_types = FALSE)\nprint_success(sprintf(\"Loaded train.csv: %s videos\", bold(as.character(nrow(train)))))\ncat(\"\\n\", magenta(\"ğŸ“‹ Columns in train.csv:\"), \"\\n\", sep = \"\")\nfor(col in names(train)) {\n  print_bullet(col, 2)\n}\n\n# Check for test.csv\nif(file.exists(file.path(base_path, \"test.csv\"))) {\n  test <- read_csv(file.path(base_path, \"test.csv\"), show_col_types = FALSE)\n  print_success(sprintf(\"Loaded test.csv: %s videos\", bold(as.character(nrow(test)))))\n} else {\n  print_warning(\"test.csv not found\")\n  test <- NULL\n}\n\n# Check for sample_submission.csv\nif(file.exists(file.path(base_path, \"sample_submission.csv\"))) {\n  sample_submission <- read_csv(file.path(base_path, \"sample_submission.csv\"), show_col_types = FALSE)\n  print_success(sprintf(\"Loaded sample_submission.csv: %s rows\", bold(as.character(nrow(sample_submission)))))\n} else {\n  print_warning(\"sample_submission.csv not found\")\n  sample_submission <- NULL\n}\n\n# ============================================\n# 2. EXPLORE DATA STRUCTURE\n# ============================================\n\nprint_header(\"STEP 2: EXPLORING DATA STRUCTURE\", 2)\n\n# Count unique labs\nunique_labs <- unique(train$lab_id)\ncat(magenta(\"ğŸ”¬ Number of unique labs:\"), bold(length(unique_labs)), \"\\n\\n\")\ncat(cyan(\"Labs Overview:\\n\"))\nfor(lab in unique_labs[1:min(10, length(unique_labs))]) {\n  video_count <- sum(train$lab_id == lab)\n  cat(sprintf(\"  %s %s: %s videos\\n\", \n              green(\"â†’\"), \n              bold(lab), \n              yellow(video_count)))\n}\nif(length(unique_labs) > 10) {\n  cat(sprintf(\"  ... and %s more labs\\n\", bold(length(unique_labs) - 10)))\n}\n\n# Check which labs have annotations\nlabs_with_annotations <- c()\nlabs_without_annotations <- c()\n\nprint_header(\"Checking Annotation Availability\", 3)\nfor(lab in unique_labs[1:min(5, length(unique_labs))]) {\n  sample_video <- train$video_id[train$lab_id == lab][1]\n  annotation_path <- file.path(base_path, \"train_annotation\", lab, paste0(sample_video, \".parquet\"))\n  \n  if(file.exists(annotation_path)) {\n    labs_with_annotations <- c(labs_with_annotations, lab)\n    cat(\"  \", green(\"âœ“\"), \" \", lab, \" \", green(\"has annotations\"), \"\\n\", sep = \"\")\n  } else {\n    labs_without_annotations <- c(labs_without_annotations, lab)\n    cat(\"  \", red(\"âœ—\"), \" \", lab, \" \", red(\"has NO annotations\"), \"\\n\", sep = \"\")\n  }\n}\n\n# ============================================\n# 3. LOAD AND ANALYZE ANNOTATIONS\n# ============================================\n\nprint_header(\"STEP 3: LOADING ANNOTATION DATA\", 2)\n\n# Function to load annotations for a specific video\nload_video_annotations <- function(lab_id, video_id) {\n  path <- file.path(base_path, \"train_annotation\", lab_id, paste0(video_id, \".parquet\"))\n  if(file.exists(path)) {\n    df <- read_parquet(path)\n    df$lab_id <- lab_id\n    df$video_id <- video_id\n    return(df)\n  }\n  return(NULL)\n}\n\n# Load sample annotations from labs that have them\nall_annotations <- list()\nn_videos_to_sample <- 5\n\ncat(cyan(sprintf(\"\\nğŸ“¥ Loading %d sample videos per lab with annotations...\\n\\n\", n_videos_to_sample)))\n\nfor(lab in labs_with_annotations) {\n  lab_videos <- train$video_id[train$lab_id == lab][1:min(n_videos_to_sample, sum(train$lab_id == lab))]\n  \n  lab_annotations <- list()\n  for(video in lab_videos) {\n    annot <- load_video_annotations(lab, video)\n    if(!is.null(annot)) {\n      lab_annotations[[length(lab_annotations) + 1]] <- annot\n    }\n  }\n  \n  if(length(lab_annotations) > 0) {\n    lab_df <- bind_rows(lab_annotations)\n    all_annotations[[lab]] <- lab_df\n    cat(\"  \", green(\"â€¢\"), \" Loaded \", bold(nrow(lab_df)), \" annotations from \", yellow(lab), \"\\n\", sep = \"\")\n  }\n}\n\n# Combine all annotations\nif(length(all_annotations) > 0) {\n  annotations_df <- bind_rows(all_annotations)\n  cat(\"\\n\")\n  print_success(sprintf(\"Total annotations loaded: %s\", bold(green(as.character(nrow(annotations_df))))))\n  cat(\"\\n\", magenta(\"ğŸ“Š Annotation columns:\"), \"\\n\", sep = \"\")\n  for(col in names(annotations_df)) {\n    print_bullet(col, 2)\n  }\n} else {\n  print_warning(\"No annotations could be loaded\")\n  annotations_df <- data.frame()\n}\n\n# ============================================\n# 4. BEHAVIOR ANALYSIS\n# ============================================\n\nif(nrow(annotations_df) > 0) {\n  print_header(\"STEP 4: BEHAVIOR ANALYSIS\", 2)\n  \n  # Behavior frequency\n  behavior_counts <- annotations_df |>\n    count(action, sort = TRUE) |>\n    mutate(percentage = n / sum(n) * 100)\n  \n  cat(cyan(\"ğŸ¯ Top 20 Most Frequent Behaviors:\\n\"))\n  cat(yellow(\"â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\\n\"))\n  for(i in 1:min(20, nrow(behavior_counts))) {\n    bar_length <- round(behavior_counts$percentage[i] / 2)\n    bar <- paste0(rep(\"â–ˆ\", bar_length), collapse = \"\")\n    cat(sprintf(\"%s %2d. %-25s %s %s (%s%%)\\n\", \n                ifelse(i <= 3, \"ğŸ¥‡ğŸ¥ˆğŸ¥‰\"[i], \"  \"),\n                i, \n                bold(behavior_counts$action[i]), \n                cyan(bar),\n                yellow(sprintf(\"%6d\", behavior_counts$n[i])), \n                green(sprintf(\"%5.2f\", behavior_counts$percentage[i]))))\n  }\n  \n  # Duration analysis\n  if(all(c(\"start_frame\", \"stop_frame\") %in% names(annotations_df))) {\n    annotations_df <- annotations_df |>\n      mutate(duration_frames = stop_frame - start_frame + 1)\n    \n    duration_stats <- annotations_df |>\n      group_by(action) |>\n      summarise(\n        mean_duration = mean(duration_frames, na.rm = TRUE),\n        median_duration = median(duration_frames, na.rm = TRUE),\n        sd_duration = sd(duration_frames, na.rm = TRUE),\n        min_duration = min(duration_frames, na.rm = TRUE),\n        max_duration = max(duration_frames, na.rm = TRUE),\n        count = n(),\n        .groups = \"drop\"\n      ) |>\n      filter(count >= 10) |>\n      arrange(desc(mean_duration))\n    \n    cat(\"\\n\", cyan(\"â±ï¸ Behavior Duration Statistics (frames):\"), \"\\n\", sep = \"\")\n    cat(yellow(\"â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\\n\"))\n    \n    # Format duration stats nicely\n    for(i in 1:min(10, nrow(duration_stats))) {\n      cat(sprintf(\"  %s %-20s Mean: %s Â± %s frames\\n\",\n                  green(\"â€¢\"),\n                  bold(duration_stats$action[i]),\n                  yellow(sprintf(\"%6.1f\", duration_stats$mean_duration[i])),\n                  cyan(sprintf(\"%5.1f\", duration_stats$sd_duration[i]))))\n    }\n  }\n  \n  # Social vs Non-social analysis\n  if(all(c(\"agent_id\", \"target_id\") %in% names(annotations_df))) {\n    annotations_df <- annotations_df |>\n      mutate(is_social = agent_id != target_id)\n    \n    social_stats <- annotations_df |>\n      group_by(action) |>\n      summarise(\n        total = n(),\n        social = sum(is_social),\n        non_social = sum(!is_social),\n        social_ratio = social / total,\n        .groups = \"drop\"\n      ) |>\n      filter(total >= 20) |>\n      arrange(desc(social_ratio))\n    \n    print_header(\"Social Behavior Analysis\", 3)\n    \n    cat(magenta(\"ğŸ‘¥ Most Social Behaviors (>80% social):\\n\"))\n    social_behaviors <- social_stats |> filter(social_ratio > 0.8)\n    if(nrow(social_behaviors) > 0) {\n      for(i in 1:min(10, nrow(social_behaviors))) {\n        cat(sprintf(\"  %s %-25s %s social (%s/%s)\\n\", \n                    green(\"â†’\"),\n                    bold(social_behaviors$action[i]),\n                    cyan(sprintf(\"%.1f%%\", social_behaviors$social_ratio[i] * 100)),\n                    yellow(social_behaviors$social[i]),\n                    social_behaviors$total[i]))\n      }\n    } else {\n      cat(\"  No highly social behaviors found\\n\")\n    }\n    \n    cat(\"\\n\", magenta(\"ğŸ­ Most Individual Behaviors (<20% social):\\n\"), sep = \"\")\n    individual_behaviors <- social_stats |> filter(social_ratio < 0.2)\n    if(nrow(individual_behaviors) > 0) {\n      for(i in 1:min(10, nrow(individual_behaviors))) {\n        cat(sprintf(\"  %s %-25s %s social (%s/%s)\\n\", \n                    green(\"â†’\"),\n                    bold(individual_behaviors$action[i]),\n                    cyan(sprintf(\"%.1f%%\", individual_behaviors$social_ratio[i] * 100)),\n                    yellow(individual_behaviors$social[i]),\n                    individual_behaviors$total[i]))\n      }\n    } else {\n      cat(\"  No highly individual behaviors found\\n\")\n    }\n  }\n}\n\n# ============================================\n# 5. TRACKING DATA EXPLORATION\n# ============================================\n\nprint_header(\"STEP 5: TRACKING DATA EXPLORATION\", 2)\n\n# Function to load tracking data\nload_video_tracking <- function(lab_id, video_id) {\n  path <- file.path(base_path, \"train_tracking\", lab_id, paste0(video_id, \".parquet\"))\n  if(file.exists(path)) {\n    return(read_parquet(path))\n  }\n  return(NULL)\n}\n\n# Load a sample tracking file\nsample_lab <- labs_with_annotations[1]\nif(length(sample_lab) > 0) {\n  sample_video <- train$video_id[train$lab_id == sample_lab][1]\n  cat(cyan(sprintf(\"\\nğŸ“ Loading tracking data for %s/%s...\\n\", sample_lab, sample_video)))\n  \n  sample_tracking <- load_video_tracking(sample_lab, sample_video)\n  \n  if(!is.null(sample_tracking)) {\n    print_success(sprintf(\"Loaded %s tracking points\", bold(as.character(nrow(sample_tracking)))))\n    cat(\"\\n\", magenta(\"Tracking columns:\"), \"\\n\", sep = \"\")\n    for(col in names(sample_tracking)) {\n      print_bullet(col, 2)\n    }\n    \n    # Analyze body parts\n    if(\"bodypart\" %in% names(sample_tracking)) {\n      bodyparts <- unique(sample_tracking$bodypart)\n      cat(\"\\n\", cyan(sprintf(\"ğŸ¦´ Body parts tracked (%d):\", length(bodyparts))), \"\\n\", sep = \"\")\n      for(bp in bodyparts) {\n        print_bullet(bp, 2)\n      }\n    }\n    \n    # Analyze mice\n    if(\"mouse_id\" %in% names(sample_tracking)) {\n      mice <- unique(sample_tracking$mouse_id)\n      cat(\"\\n\", cyan(\"ğŸ­ Mice in video: \"), yellow(paste(mice, collapse = \", \")), \"\\n\", sep = \"\")\n    }\n    \n    # Basic movement statistics\n    if(all(c(\"x\", \"y\", \"video_frame\", \"mouse_id\") %in% names(sample_tracking))) {\n      movement_stats <- sample_tracking |>\n        group_by(mouse_id) |>\n        summarise(\n          mean_x = mean(x, na.rm = TRUE),\n          mean_y = mean(y, na.rm = TRUE),\n          sd_x = sd(x, na.rm = TRUE),\n          sd_y = sd(y, na.rm = TRUE),\n          range_x = max(x, na.rm = TRUE) - min(x, na.rm = TRUE),\n          range_y = max(y, na.rm = TRUE) - min(y, na.rm = TRUE),\n          .groups = \"drop\"\n        )\n      \n      cat(\"\\n\", cyan(\"ğŸ“Š Movement Statistics by Mouse:\"), \"\\n\", sep = \"\")\n      cat(yellow(\"â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\\n\"))\n      for(i in 1:nrow(movement_stats)) {\n        cat(sprintf(\"  %s Mouse %s: X-range=%s, Y-range=%s\\n\",\n                    green(\"â€¢\"),\n                    bold(movement_stats$mouse_id[i]),\n                    yellow(sprintf(\"%.1f\", movement_stats$range_x[i])),\n                    yellow(sprintf(\"%.1f\", movement_stats$range_y[i]))))\n      }\n    }\n  }\n}\n\n# ============================================\n# 6. CREATE AND DISPLAY ALL VISUALIZATIONS\n# ============================================\n\nif((SAVE_EDA_FILES || DISPLAY_PLOTS) && nrow(annotations_df) > 0) {\n  print_header(\"STEP 6: CREATING VISUALIZATIONS\", 2)\n  \n  # Suppress automatic plot file creation if not saving\n  if(!SAVE_EDA_FILES && DISPLAY_PLOTS) {\n    pdf(NULL)\n  }\n  \n  if(!exists(\"behavior_counts\") || nrow(behavior_counts) == 0) {\n    print_warning(\"No behavior data to visualize.\")\n  } else {\n    \n    # ====== Plot 1: Behavior frequency ======\n    cat(\"\\n\", cyan(\"ğŸ“Š Plot 1: Behavior Frequency\"), \"\\n\", sep = \"\")\n    cat(yellow(\"â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\\n\"))\n    \n    tryCatch({\n      plot1_data <- behavior_counts |> head(15)\n      \n      p1 <- ggplot(plot1_data, aes(x = reorder(action, n), y = n)) +\n        geom_bar(stat = \"identity\", fill = \"#2E86AB\", alpha = 0.9) +\n        geom_text(aes(label = scales::comma(n)), hjust = -0.1, size = 5, color = \"#333\") +\n        coord_flip() +\n        labs(title = \"MABe Challenge: Top 15 Most Frequent Behaviors\",\n             subtitle = \"Based on sample annotation data\",\n             x = \"Behavior\",\n             y = \"Count\") +\n        scale_y_continuous(labels = scales::comma, expand = c(0, 0, 0.1, 0)) +\n        theme_minimal(base_size = 18) +\n        theme(plot.title = element_text(size = 22, face = \"bold\", color = \"#1A1A2E\"),\n              plot.subtitle = element_text(size = 16, color = \"#666\"),\n              axis.text = element_text(size = 14),\n              axis.title = element_text(size = 16, face = \"bold\"),\n              panel.grid.major.y = element_blank(),\n              panel.grid.minor = element_blank(),\n              plot.background = element_rect(fill = \"#FAFAFA\", color = NA),\n              panel.background = element_rect(fill = \"#FAFAFA\", color = NA))\n      \n      if(SAVE_EDA_FILES) {\n        ggsave(\"EDA/MABe_behavior_frequency.png\", p1, width = 14, height = 10, dpi = 150)\n        print_success(\"Saved to: EDA/MABe_behavior_frequency.png\")\n      }\n      \n      if(DISPLAY_PLOTS) {\n        print(p1)\n        cat(\"\\n\")\n      }\n      rm(p1, plot1_data)\n    }, error = function(e) {\n      print_warning(sprintf(\"Error creating behavior frequency plot: %s\", e$message))\n    })\n    \n    # ====== Plot 2: Duration distribution ======\n    if(exists(\"duration_stats\") && nrow(duration_stats) > 0) {\n      cat(\"\\n\", cyan(\"ğŸ“Š Plot 2: Behavior Duration\"), \"\\n\", sep = \"\")\n      cat(yellow(\"â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\\n\"))\n      \n      tryCatch({\n        plot2_data <- duration_stats |> head(10)\n        \n        p2 <- ggplot(plot2_data, aes(x = reorder(action, mean_duration), y = mean_duration)) +\n          geom_bar(stat = \"identity\", fill = \"#F71735\", alpha = 0.8) +\n          geom_errorbar(aes(ymin = pmax(0, mean_duration - sd_duration), \n                            ymax = mean_duration + sd_duration),\n                        width = 0.3, color = \"#333\", size = 1) +\n          geom_text(aes(label = sprintf(\"%.1f\", mean_duration)), \n                    hjust = -0.2, size = 5, color = \"#333\") +\n          coord_flip() +\n          labs(title = \"MABe Challenge: Behavior Duration Analysis\",\n               subtitle = \"Mean duration Â± standard deviation (in frames)\",\n               x = \"Behavior\",\n               y = \"Duration (frames)\") +\n          theme_minimal(base_size = 18) +\n          theme(plot.title = element_text(size = 22, face = \"bold\", color = \"#1A1A2E\"),\n                plot.subtitle = element_text(size = 16, color = \"#666\"),\n                axis.text = element_text(size = 14),\n                axis.title = element_text(size = 16, face = \"bold\"),\n                panel.grid.major.y = element_blank(),\n                plot.background = element_rect(fill = \"#FAFAFA\", color = NA),\n                panel.background = element_rect(fill = \"#FAFAFA\", color = NA))\n        \n        if(SAVE_EDA_FILES) {\n          ggsave(\"EDA/MABe_behavior_duration.png\", p2, width = 14, height = 10, dpi = 150)\n          print_success(\"Saved to: EDA/MABe_behavior_duration.png\")\n        }\n        \n        if(DISPLAY_PLOTS) {\n          print(p2)\n          cat(\"\\n\")\n        }\n        rm(p2, plot2_data)\n      }, error = function(e) {\n        print_warning(sprintf(\"Error creating duration plot: %s\", e$message))\n      })\n    }\n    \n    # ====== Plot 3: Lab distribution ======\n    cat(\"\\n\", cyan(\"ğŸ“Š Plot 3: Lab Distribution\"), \"\\n\", sep = \"\")\n    cat(yellow(\"â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\\n\"))\n    \n    tryCatch({\n      lab_dist <- train |>\n        count(lab_id) |>\n        arrange(desc(n)) |>\n        head(10)\n      \n      p3 <- ggplot(lab_dist, aes(x = reorder(lab_id, n), y = n)) +\n        geom_bar(stat = \"identity\", fill = \"#8FE13E\", alpha = 0.9) +\n        geom_text(aes(label = n), hjust = -0.2, size = 5, color = \"#333\", fontface = \"bold\") +\n        coord_flip() +\n        labs(title = \"MABe Challenge: Videos per Laboratory\",\n             subtitle = \"Top 10 labs by video count\",\n             x = \"Lab ID\",\n             y = \"Number of Videos\") +\n        scale_y_continuous(expand = c(0, 0, 0.1, 0)) +\n        theme_minimal(base_size = 18) +\n        theme(plot.title = element_text(size = 22, face = \"bold\", color = \"#1A1A2E\"),\n              plot.subtitle = element_text(size = 16, color = \"#666\"),\n              axis.text = element_text(size = 14),\n              axis.title = element_text(size = 16, face = \"bold\"),\n              panel.grid.major.y = element_blank(),\n              plot.background = element_rect(fill = \"#FAFAFA\", color = NA),\n              panel.background = element_rect(fill = \"#FAFAFA\", color = NA))\n      \n      if(SAVE_EDA_FILES) {\n        ggsave(\"EDA/MABe_lab_distribution.png\", p3, width = 14, height = 10, dpi = 150)\n        print_success(\"Saved to: EDA/MABe_lab_distribution.png\")\n      }\n      \n      if(DISPLAY_PLOTS) {\n        print(p3)\n        cat(\"\\n\")\n      }\n      rm(p3, lab_dist)\n    }, error = function(e) {\n      print_warning(sprintf(\"Error creating lab distribution plot: %s\", e$message))\n    })\n    \n    # ====== Plot 4: Social vs non-social behaviors ======\n    if(exists(\"social_stats\") && nrow(social_stats) > 0) {\n      cat(\"\\n\", cyan(\"ğŸ“Š Plot 4: Social vs Non-Social Behaviors\"), \"\\n\", sep = \"\")\n      cat(yellow(\"â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\\n\"))\n      \n      tryCatch({\n        social_plot_data <- social_stats |>\n          head(15) |>\n          pivot_longer(cols = c(social, non_social), \n                       names_to = \"type\", \n                       values_to = \"count\") |>\n          mutate(type = ifelse(type == \"social\", \"Social\", \"Non-Social\"))\n        \n        p4 <- ggplot(social_plot_data, \n                     aes(x = reorder(action, social_ratio), y = count, fill = type)) +\n          geom_bar(stat = \"identity\", position = \"stack\", alpha = 0.9) +\n          coord_flip() +\n          labs(title = \"MABe Challenge: Social vs Non-Social Behaviors\",\n               subtitle = \"Behavioral interactions between mice\",\n               x = \"Behavior\",\n               y = \"Count\",\n               fill = \"Interaction Type\") +\n          scale_fill_manual(values = c(\"Social\" = \"#E74C3C\", \"Non-Social\" = \"#3498DB\")) +\n          theme_minimal(base_size = 18) +\n          theme(plot.title = element_text(size = 22, face = \"bold\", color = \"#1A1A2E\"),\n                plot.subtitle = element_text(size = 16, color = \"#666\"),\n                axis.text = element_text(size = 14),\n                axis.title = element_text(size = 16, face = \"bold\"),\n                legend.position = \"bottom\",\n                legend.title = element_text(size = 14, face = \"bold\"),\n                legend.text = element_text(size = 12),\n                panel.grid.major.y = element_blank(),\n                plot.background = element_rect(fill = \"#FAFAFA\", color = NA),\n                panel.background = element_rect(fill = \"#FAFAFA\", color = NA))\n        \n        if(SAVE_EDA_FILES) {\n          ggsave(\"EDA/MABe_social_behaviors.png\", p4, width = 14, height = 10, dpi = 150)\n          print_success(\"Saved to: EDA/MABe_social_behaviors.png\")\n        }\n        \n        if(DISPLAY_PLOTS) {\n          print(p4)\n          cat(\"\\n\")\n        }\n        rm(p4, social_plot_data)\n      }, error = function(e) {\n        print_warning(sprintf(\"Error creating social behaviors plot: %s\", e$message))\n      })\n    }\n    \n    # ====== Plot 5: Behavior percentage distribution ======\n    cat(\"\\n\", cyan(\"ğŸ“Š Plot 5: Behavior Percentage Distribution\"), \"\\n\", sep = \"\")\n    cat(yellow(\"â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\\n\"))\n    \n    tryCatch({\n      plot5_data <- behavior_counts |> head(10)\n      \n      p5 <- ggplot(plot5_data, aes(x = reorder(action, percentage), y = percentage)) +\n        geom_bar(stat = \"identity\", fill = \"#FF6B6B\", alpha = 0.9) +\n        geom_text(aes(label = sprintf(\"%.1f%%\", percentage)), \n                  hjust = -0.1, size = 5, color = \"#333\", fontface = \"bold\") +\n        coord_flip() +\n        labs(title = \"MABe Challenge: Behavior Distribution\",\n             subtitle = \"Percentage of total annotations\",\n             x = \"Behavior\",\n             y = \"Percentage (%)\") +\n        scale_y_continuous(expand = c(0, 0, 0.05, 0)) +\n        theme_minimal(base_size = 18) +\n        theme(plot.title = element_text(size = 22, face = \"bold\", color = \"#1A1A2E\"),\n              plot.subtitle = element_text(size = 16, color = \"#666\"),\n              axis.text = element_text(size = 14),\n              axis.title = element_text(size = 16, face = \"bold\"),\n              panel.grid.major.y = element_blank(),\n              plot.background = element_rect(fill = \"#FAFAFA\", color = NA),\n              panel.background = element_rect(fill = \"#FAFAFA\", color = NA))\n      \n      if(SAVE_EDA_FILES) {\n        ggsave(\"EDA/MABe_behavior_percentage.png\", p5, width = 14, height = 10, dpi = 150)\n        print_success(\"Saved to: EDA/MABe_behavior_percentage.png\")\n      }\n      \n      if(DISPLAY_PLOTS) {\n        print(p5)\n        cat(\"\\n\")\n      }\n      rm(p5, plot5_data)\n    }, error = function(e) {\n      print_warning(sprintf(\"Error creating percentage distribution plot: %s\", e$message))\n    })\n    \n    # ====== Plot 6: Duration vs Frequency scatter plot ======\n    if(exists(\"duration_stats\") && exists(\"behavior_counts\") && \n       nrow(duration_stats) > 0 && nrow(behavior_counts) > 0) {\n      cat(\"\\n\", cyan(\"ğŸ“Š Plot 6: Duration vs Frequency Analysis\"), \"\\n\", sep = \"\")\n      cat(yellow(\"â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\\n\"))\n      \n      tryCatch({\n        duration_freq <- duration_stats |>\n          inner_join(behavior_counts, by = \"action\") |>\n          head(20)\n        \n        if(nrow(duration_freq) > 0) {\n          p6 <- ggplot(duration_freq, \n                       aes(x = n, y = mean_duration, label = action)) +\n            geom_point(size = 6, color = \"#4ECDC4\", alpha = 0.8) +\n            geom_text(nudge_y = 5, size = 4, check_overlap = TRUE, color = \"#333\") +\n            scale_x_log10(labels = scales::comma) +\n            labs(title = \"MABe Challenge: Behavior Duration vs Frequency\",\n                 subtitle = \"Relationship between how often behaviors occur and how long they last\",\n                 x = \"Frequency (log scale)\",\n                 y = \"Mean Duration (frames)\") +\n            theme_minimal(base_size = 18) +\n            theme(plot.title = element_text(size = 22, face = \"bold\", color = \"#1A1A2E\"),\n                  plot.subtitle = element_text(size = 16, color = \"#666\"),\n                  axis.text = element_text(size = 14),\n                  axis.title = element_text(size = 16, face = \"bold\"),\n                  panel.grid.minor = element_blank(),\n                  plot.background = element_rect(fill = \"#FAFAFA\", color = NA),\n                  panel.background = element_rect(fill = \"#FAFAFA\", color = NA))\n          \n          if(SAVE_EDA_FILES) {\n            ggsave(\"EDA/MABe_duration_vs_frequency.png\", p6, width = 14, height = 10, dpi = 150)\n            print_success(\"Saved to: EDA/MABe_duration_vs_frequency.png\")\n          }\n          \n          if(DISPLAY_PLOTS) {\n            print(p6)\n            cat(\"\\n\")\n          }\n          rm(p6)\n        }\n        rm(duration_freq)\n      }, error = function(e) {\n        print_warning(sprintf(\"Error creating duration vs frequency plot: %s\", e$message))\n      })\n    }\n    \n    if(SAVE_EDA_FILES) {\n      cat(\"\\n\")\n      print_success(\"All visualizations completed\")\n    }\n  }\n  \n  # Turn off NULL pdf device if we used it\n  if(!SAVE_EDA_FILES && DISPLAY_PLOTS && !is.null(dev.list())) {\n    tryCatch(dev.off(), error = function(e) {})\n  }\n}\n\n# ============================================\n# 7. SUMMARY STATISTICS\n# ============================================\n\nprint_header(\"STEP 7: SUMMARY STATISTICS\", 2)\n\nsummary_stats <- list(\n  \"Total videos in training\" = nrow(train),\n  \"Total videos in test\" = ifelse(!is.null(test), nrow(test), 0),\n  \"Number of labs\" = length(unique_labs),\n  \"Labs with annotations\" = length(labs_with_annotations),\n  \"Labs without annotations\" = length(labs_without_annotations),\n  \"Total annotations loaded\" = nrow(annotations_df)\n)\n\nif(nrow(annotations_df) > 0) {\n  summary_stats[[\"Unique behaviors\"]] <- n_distinct(annotations_df$action)\n  summary_stats[[\"Most frequent behavior\"]] <- behavior_counts$action[1]\n  summary_stats[[\"Most frequent count\"]] <- behavior_counts$n[1]\n  summary_stats[[\"Most frequent percentage\"]] <- sprintf(\"%.2f%%\", behavior_counts$percentage[1])\n}\n\n# Save summary to file only if EDA files are enabled\nif(SAVE_EDA_FILES) {\n  summary_df <- data.frame(\n    Metric = names(summary_stats),\n    Value = unlist(summary_stats)\n  )\n  write_csv(summary_df, \"EDA/MABe_summary_statistics.csv\")\n  print_success(\"Summary statistics saved to: EDA/MABe_summary_statistics.csv\")\n}\n\nif(VERBOSE_OUTPUT) {\n  cat(\"\\n\", magenta(\"ğŸ“Š Summary Statistics:\"), \"\\n\", sep = \"\")\n  cat(yellow(\"â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\\n\"))\n  for(name in names(summary_stats)) {\n    cat(sprintf(\"  %s %-30s: %s\\n\", green(\"â€¢\"), name, bold(summary_stats[[name]])))\n  }\n}\n\n# ============================================\n# 8. IMPROVED BASELINE PREDICTION\n# ============================================\n\nprint_header(\"STEP 8: ADVANCED BASELINE PREDICTION\", 2)\n\n# ==== HELPER FUNCTIONS FOR ADVANCED BASELINE ====\n\n# Function to compute action priors with Laplace smoothing\ncompute_action_priors <- function(annotations_df, eps = 1.0) {\n  if(nrow(annotations_df) == 0) {\n    return(list(global = data.frame(), by_lab = data.frame()))\n  }\n  \n  # Global priors based on frame counts\n  global_priors <- annotations_df %>%\n    mutate(frame_count = stop_frame - start_frame) %>%\n    group_by(action) %>%\n    summarise(\n      total_frames = sum(frame_count, na.rm = TRUE) + eps,\n      .groups = \"drop\"\n    ) %>%\n    mutate(prior = total_frames / sum(total_frames))\n  \n  # Lab-specific priors\n  lab_priors <- annotations_df %>%\n    mutate(frame_count = stop_frame - start_frame) %>%\n    group_by(lab_id, action) %>%\n    summarise(\n      total_frames = sum(frame_count, na.rm = TRUE) + eps,\n      .groups = \"drop\"\n    ) %>%\n    group_by(lab_id) %>%\n    mutate(prior = total_frames / sum(total_frames)) %>%\n    ungroup()\n  \n  # Also compute median durations\n  global_durations <- annotations_df %>%\n    mutate(duration = stop_frame - start_frame) %>%\n    group_by(action) %>%\n    summarise(\n      median_duration = median(duration, na.rm = TRUE),\n      .groups = \"drop\"\n    )\n  \n  lab_durations <- annotations_df %>%\n    mutate(duration = stop_frame - start_frame) %>%\n    group_by(lab_id, action) %>%\n    summarise(\n      median_duration = median(duration, na.rm = TRUE),\n      .groups = \"drop\"\n    )\n  \n  return(list(\n    global = global_priors,\n    by_lab = lab_priors,\n    global_durations = global_durations,\n    lab_durations = lab_durations\n  ))\n}\n\n# Function to compute proximity windows from tracking data\ncompute_proximity_windows <- function(tracking_df, agent_id, target_id, \n                                     dist_quantile = 0.4, \n                                     speed_quantile = 0.6,\n                                     min_len = 10) {\n  # Normalize mouse IDs\n  agent_id <- gsub(\"mouse\", \"\", as.character(agent_id))\n  target_id <- gsub(\"mouse\", \"\", as.character(target_id))\n  \n  # Get agent and target tracking data\n  agent_data <- tracking_df %>% \n    filter(mouse_id == agent_id | mouse_id == paste0(\"mouse\", agent_id)) %>%\n    group_by(video_frame) %>%\n    slice(1) %>%  # Take first if multiple\n    ungroup() %>%\n    arrange(video_frame) %>%\n    select(video_frame, x_a = x, y_a = y)\n  \n  target_data <- tracking_df %>% \n    filter(mouse_id == target_id | mouse_id == paste0(\"mouse\", target_id)) %>%\n    group_by(video_frame) %>%\n    slice(1) %>%\n    ungroup() %>%\n    arrange(video_frame) %>%\n    select(video_frame, x_t = x, y_t = y)\n  \n  if(nrow(agent_data) == 0 || nrow(target_data) == 0) {\n    return(NULL)\n  }\n  \n  # Merge on common frames\n  merged <- inner_join(agent_data, target_data, by = \"video_frame\")\n  \n  if(nrow(merged) < 2) {\n    return(NULL)\n  }\n  \n  # Compute features\n  merged <- merged %>%\n    arrange(video_frame) %>%\n    mutate(\n      # Distance between mice\n      dist = sqrt((x_a - x_t)^2 + (y_a - y_t)^2),\n      # Speed of each mouse (frame-to-frame displacement)\n      dx_a = x_a - lag(x_a, default = first(x_a)),\n      dy_a = y_a - lag(y_a, default = first(y_a)),\n      dx_t = x_t - lag(x_t, default = first(x_t)),\n      dy_t = y_t - lag(y_t, default = first(y_t)),\n      speed_a = sqrt(dx_a^2 + dy_a^2),\n      speed_t = sqrt(dx_t^2 + dy_t^2),\n      # Relative speed (agent relative to target)\n      rel_speed = speed_a - speed_t,\n      # Change in distance (approaching < 0)\n      ddist = dist - lag(dist, default = first(dist))\n    )\n  \n  # Compute thresholds\n  dist_thresh <- quantile(merged$dist, dist_quantile, na.rm = TRUE)\n  speed_thresh <- quantile(merged$rel_speed, speed_quantile, na.rm = TRUE)\n  ddist_thresh <- quantile(merged$ddist, 0.4, na.rm = TRUE)\n  \n  # Create proximity condition\n  # Close together OR (moving fast relative to each other AND approaching)\n  merged <- merged %>%\n    mutate(\n      is_proximate = (dist <= dist_thresh) | \n                     ((rel_speed >= speed_thresh) & (ddist <= ddist_thresh))\n    )\n  \n  # Convert boolean mask to windows\n  windows <- list()\n  frames <- merged$video_frame\n  mask <- merged$is_proximate\n  \n  if(sum(mask, na.rm = TRUE) == 0) {\n    return(NULL)\n  }\n  \n  # Find continuous runs of TRUE values\n  run_start <- NULL\n  for(i in 1:length(mask)) {\n    if(!is.na(mask[i]) && mask[i]) {\n      if(is.null(run_start)) {\n        run_start <- frames[i]\n      }\n    } else {\n      if(!is.null(run_start)) {\n        run_end <- frames[i - 1]\n        if(run_end - run_start >= min_len) {\n          windows[[length(windows) + 1]] <- c(run_start, run_end)\n        }\n        run_start <- NULL\n      }\n    }\n  }\n  \n  # Handle last run\n  if(!is.null(run_start)) {\n    run_end <- frames[length(frames)]\n    if(run_end - run_start >= min_len) {\n      windows[[length(windows) + 1]] <- c(run_start, run_end)\n    }\n  }\n  \n  # Merge overlapping windows\n  if(length(windows) > 1) {\n    windows <- merge_windows(windows)\n  }\n  \n  return(windows)\n}\n\n# Helper to merge overlapping or nearby windows\nmerge_windows <- function(windows, gap = 5) {\n  if(length(windows) <= 1) return(windows)\n  \n  # Convert to matrix for easier manipulation\n  win_mat <- do.call(rbind, windows)\n  win_mat <- win_mat[order(win_mat[,1]), , drop = FALSE]\n  \n  merged <- list()\n  current <- win_mat[1, ]\n  \n  for(i in 2:nrow(win_mat)) {\n    if(win_mat[i, 1] <= current[2] + gap) {\n      # Overlapping or close - extend current window\n      current[2] <- max(current[2], win_mat[i, 2])\n    } else {\n      # Not overlapping - save current and start new\n      merged[[length(merged) + 1]] <- current\n      current <- win_mat[i, ]\n    }\n  }\n  merged[[length(merged) + 1]] <- current\n  \n  return(merged)\n}\n\n# Advanced baseline prediction function\ncreate_advanced_baseline <- function(dataset, traintest = \"test\", \n                                   action_priors = NULL,\n                                   use_windows = TRUE,\n                                   min_segment_len = 10,\n                                   gap_close = 5,\n                                   rare_action_cap = 0.02) {\n  \n  submission_list <- list()\n  canonical_order <- c(\"approach\", \"avoid\", \"chase\", \"chaseattack\", \n                      \"attack\", \"mount\", \"submit\")\n  \n  for(i in 1:nrow(dataset)) {\n    row <- dataset[i, ]\n    \n    # Skip MABe22 labs\n    if(startsWith(row$lab_id, \"MABe22\")) next\n    \n    lab_id <- row$lab_id\n    video_id <- row$video_id\n    \n    # Load tracking data\n    path <- file.path(base_path, paste0(traintest, \"_tracking\"), \n                      lab_id, paste0(video_id, \".parquet\"))\n    \n    if(!file.exists(path)) next\n    \n    vid <- read_parquet(path)\n    \n    # Parse behaviors\n    behaviors_str <- row$behaviors_labeled\n    if(is.na(behaviors_str) || behaviors_str == \"\") next\n    \n    behaviors_str_clean <- gsub(\"\\\\[|\\\\]|'|\\\"|\\\\s\", \"\", behaviors_str)\n    behaviors <- strsplit(behaviors_str_clean, \",\")[[1]]\n    \n    if(length(behaviors) %% 3 != 0) next\n    \n    behavior_df <- data.frame(\n      agent = behaviors[seq(1, length(behaviors), 3)],\n      target = behaviors[seq(2, length(behaviors), 3)],\n      action = behaviors[seq(3, length(behaviors), 3)],\n      stringsAsFactors = FALSE\n    ) %>% unique()\n    \n    if(nrow(behavior_df) == 0) next\n    \n    # Get frame range\n    start_frame <- min(vid$video_frame)\n    stop_frame <- max(vid$video_frame) + 1\n    total_frames <- stop_frame - start_frame\n    \n    # Process each unique agent-target pair\n    unique_pairs <- unique(behavior_df[, c(\"agent\", \"target\")])\n    \n    for(pair_idx in 1:nrow(unique_pairs)) {\n      agent <- unique_pairs$agent[pair_idx]\n      target <- unique_pairs$target[pair_idx]\n      \n      # Get actions for this pair\n      pair_actions <- behavior_df %>%\n        filter(agent == !!agent, target == !!target) %>%\n        pull(action) %>% \n        unique()\n      \n      n_actions <- length(pair_actions)\n      if(n_actions == 0) next\n      \n      # Get proximity windows if enabled\n      windows <- NULL\n      if(use_windows && total_frames > 50) {\n        tryCatch({\n          windows <- compute_proximity_windows(vid, agent, target)\n        }, error = function(e) {\n          windows <- NULL\n        })\n      }\n      \n      # Default to full video if no windows found\n      if(is.null(windows) || length(windows) == 0) {\n        windows <- list(c(start_frame, stop_frame))\n      }\n      \n      # Get action weights from priors\n      weights <- rep(1/n_actions, n_actions)\n      durations <- rep(30, n_actions)  # Default duration\n      \n      if(!is.null(action_priors)) {\n        # Try lab-specific priors first\n        if(nrow(action_priors$by_lab) > 0) {\n          lab_priors <- action_priors$by_lab %>%\n            filter(lab_id == !!lab_id)\n          \n          if(nrow(lab_priors) > 0) {\n            for(j in 1:length(pair_actions)) {\n              action <- pair_actions[j]\n              prior_row <- lab_priors %>% filter(action == !!action)\n              if(nrow(prior_row) > 0) {\n                weights[j] <- prior_row$prior[1]\n              }\n            }\n          }\n        }\n        \n        # Fall back to global priors if no lab-specific found\n        if(sum(weights == 1/n_actions) == n_actions && nrow(action_priors$global) > 0) {\n          for(j in 1:length(pair_actions)) {\n            action <- pair_actions[j]\n            prior_row <- action_priors$global %>% filter(action == !!action)\n            if(nrow(prior_row) > 0) {\n              weights[j] <- prior_row$prior[1]\n            }\n          }\n        }\n        \n        # Get median durations\n        if(nrow(action_priors$global_durations) > 0) {\n          for(j in 1:length(pair_actions)) {\n            action <- pair_actions[j]\n            dur_row <- action_priors$global_durations %>% filter(action == !!action)\n            if(nrow(dur_row) > 0 && !is.na(dur_row$median_duration[1])) {\n              durations[j] <- max(10, dur_row$median_duration[1])\n            }\n          }\n        }\n      }\n      \n      # Cap rare actions\n      weights[weights < rare_action_cap] <- min(weights[weights < rare_action_cap], rare_action_cap)\n      \n      # Normalize weights\n      weights <- weights / sum(weights)\n      \n      # Order actions by canonical order and weight\n      action_order <- order(\n        match(pair_actions, canonical_order, nomatch = 99),\n        -weights\n      )\n      pair_actions <- pair_actions[action_order]\n      weights <- weights[action_order]\n      durations <- durations[action_order]\n      \n      # Allocate segments within windows\n      for(window in windows) {\n        window_start <- window[1]\n        window_end <- window[2]\n        window_length <- window_end - window_start\n        \n        if(window_length < min_segment_len) next\n        \n        # Allocate proportionally but respect minimum durations\n        allocated <- 0\n        segments <- list()\n        \n        for(j in 1:length(pair_actions)) {\n          if(allocated >= window_length) break\n          \n          # Desired length based on weight and median duration\n          desired_len <- max(\n            weights[j] * window_length,\n            min(durations[j], window_length - allocated)\n          )\n          desired_len <- round(desired_len)\n          \n          # Ensure minimum length\n          if(desired_len < min_segment_len && j == length(pair_actions)) {\n            # Last action - give it remaining frames if enough\n            if(window_length - allocated >= min_segment_len) {\n              desired_len <- window_length - allocated\n            } else {\n              next\n            }\n          } else if(desired_len < min_segment_len) {\n            desired_len <- min_segment_len\n          }\n          \n          # Don't exceed window\n          actual_len <- min(desired_len, window_length - allocated)\n          \n          if(actual_len >= min_segment_len) {\n            segments[[length(segments) + 1]] <- list(\n              action = pair_actions[j],\n              start = window_start + allocated,\n              end = window_start + allocated + actual_len\n            )\n            allocated <- allocated + actual_len\n          }\n        }\n        \n        # Add segments to submission\n        for(seg in segments) {\n          submission_list[[length(submission_list) + 1]] <- data.frame(\n            video_id = as.numeric(video_id),\n            agent_id = paste0(\"mouse\", gsub(\"mouse\", \"\", agent)),\n            target_id = paste0(\"mouse\", gsub(\"mouse\", \"\", target)),\n            action = seg$action,\n            start_frame = as.integer(seg$start),\n            stop_frame = as.integer(seg$end),\n            stringsAsFactors = FALSE\n          )\n        }\n      }\n    }\n  }\n  \n  if(length(submission_list) > 0) {\n    submission_df <- bind_rows(submission_list)\n    \n    # Post-process: merge close segments of same action\n    submission_df <- submission_df %>%\n      arrange(video_id, agent_id, target_id, action, start_frame) %>%\n      group_by(video_id, agent_id, target_id, action) %>%\n      mutate(\n        # Group segments that are close together\n        new_group = cumsum(c(1, diff(start_frame) > gap_close))\n      ) %>%\n      group_by(video_id, agent_id, target_id, action, new_group) %>%\n      summarise(\n        start_frame = min(start_frame),\n        stop_frame = max(stop_frame),\n        .groups = \"drop\"\n      ) %>%\n      select(-new_group) %>%\n      # Remove very short segments after merging\n      filter(stop_frame - start_frame >= min_segment_len)\n    \n    return(submission_df)\n  } else {\n    return(data.frame())\n  }\n}\n\ncat(cyan(\"ğŸ“ˆ Strategy:\"), \"Advanced baseline with proximity windows and action priors\\n\")\ncat(\"   - Uses frequency-based action priors with Laplace smoothing\\n\")\ncat(\"   - Detects proximity windows from tracking data\\n\")\ncat(\"   - Allocates segments based on historical durations\\n\")\ncat(\"   - Post-processes to merge fragmented predictions\\n\")\n\n# ============================================\n# 9. VALIDATION WITH F1 SCORE CALCULATION\n# ============================================\n\nprint_header(\"STEP 9: VALIDATION ON TRAINING DATA\", 2)\n\n# F1 Score Implementation\ncalculate_f1_score <- function(solution_df, submission_df, beta = 1) {\n  \n  single_lab_f1 <- function(lab_solution, lab_submission, beta = 1) {\n    label_frames <- list()\n    prediction_frames <- list()\n    \n    # Build label frames\n    for(i in 1:nrow(lab_solution)) {\n      row <- lab_solution[i,]\n      key <- paste(row$video_id, row$agent_id, row$target_id, row$action, sep = \"_\")\n      frames <- seq(row$start_frame, row$stop_frame - 1)\n      if(is.null(label_frames[[key]])) {\n        label_frames[[key]] <- frames\n      } else {\n        label_frames[[key]] <- c(label_frames[[key]], frames)\n      }\n    }\n    \n    # Build prediction frames\n    for(i in 1:nrow(lab_submission)) {\n      row <- lab_submission[i,]\n      key <- paste(row$video_id, row$agent_id, row$target_id, row$action, sep = \"_\")\n      frames <- seq(row$start_frame, row$stop_frame - 1)\n      if(is.null(prediction_frames[[key]])) {\n        prediction_frames[[key]] <- frames\n      } else {\n        prediction_frames[[key]] <- c(prediction_frames[[key]], frames)\n      }\n    }\n    \n    # Calculate metrics for each action\n    actions <- unique(c(\n      sapply(names(label_frames), function(x) tail(strsplit(x, \"_\")[[1]], 1)),\n      sapply(names(prediction_frames), function(x) tail(strsplit(x, \"_\")[[1]], 1))\n    ))\n    \n    action_f1s <- numeric()\n    \n    for(action in actions) {\n      tp <- 0\n      fp <- 0\n      fn <- 0\n      \n      label_keys <- names(label_frames)[grepl(paste0(\"_\", action, \"$\"), names(label_frames))]\n      pred_keys <- names(prediction_frames)[grepl(paste0(\"_\", action, \"$\"), names(prediction_frames))]\n      all_keys <- unique(c(label_keys, pred_keys))\n      \n      for(key in all_keys) {\n        label_set <- label_frames[[key]]\n        pred_set <- prediction_frames[[key]]\n        \n        if(!is.null(label_set) && !is.null(pred_set)) {\n          tp <- tp + length(intersect(label_set, pred_set))\n          fp <- fp + length(setdiff(pred_set, label_set))\n          fn <- fn + length(setdiff(label_set, pred_set))\n        } else if(!is.null(label_set)) {\n          fn <- fn + length(label_set)\n        } else if(!is.null(pred_set)) {\n          fp <- fp + length(pred_set)\n        }\n      }\n      \n      if(tp + fn + fp == 0) {\n        f1 <- 0\n      } else {\n        f1 <- (1 + beta^2) * tp / ((1 + beta^2) * tp + beta^2 * fn + fp)\n      }\n      action_f1s <- c(action_f1s, f1)\n    }\n    \n    return(mean(action_f1s))\n  }\n  \n  # Main scoring\n  labs <- unique(solution_df$lab_id)\n  lab_scores <- numeric()\n  \n  for(lab in labs) {\n    lab_solution <- solution_df[solution_df$lab_id == lab,]\n    lab_videos <- unique(lab_solution$video_id)\n    lab_submission <- submission_df[submission_df$video_id %in% lab_videos,]\n    \n    if(nrow(lab_submission) > 0 && nrow(lab_solution) > 0) {\n      lab_score <- single_lab_f1(lab_solution, lab_submission, beta)\n      lab_scores <- c(lab_scores, lab_score)\n    }\n  }\n  \n  return(mean(lab_scores))\n}\n\n# Create solution dataframe\ncreate_solution_df <- function(dataset) {\n  solution_list <- list()\n  \n  for(i in 1:nrow(dataset)) {\n    row <- dataset[i,]\n    \n    if(startsWith(row$lab_id, \"MABe22\")) next\n    \n    annot_path <- file.path(base_path, \"train_annotation\", \n                            row$lab_id, paste0(row$video_id, \".parquet\"))\n    \n    if(file.exists(annot_path)) {\n      annot <- read_parquet(annot_path)\n      annot$lab_id <- row$lab_id\n      annot$video_id <- row$video_id\n      annot$behaviors_labeled <- row$behaviors_labeled\n      annot$agent_id <- paste0(\"mouse\", annot$agent_id)\n      annot$target_id <- paste0(\"mouse\", annot$target_id)\n      solution_list[[length(solution_list) + 1]] <- annot\n    }\n  }\n  \n  if(length(solution_list) > 0) {\n    return(bind_rows(solution_list))\n  } else {\n    return(data.frame())\n  }\n}\n\n# Run validation with advanced baseline\ntrain_subset <- train[!startsWith(train$lab_id, \"MABe22_\"), ]\n\nif(nrow(train_subset) > 0) {\n  cat(cyan(\"ğŸ“¥ Creating solution from training annotations...\\n\"))\n  solution_df <- create_solution_df(train_subset)\n  print_success(sprintf(\"Loaded %s annotations for validation\", bold(as.character(nrow(solution_df)))))\n  \n  cat(\"\\n\", cyan(\"ğŸ”¬ Computing action priors from training data...\"), \"\\n\", sep = \"\")\n  action_priors <- compute_action_priors(solution_df, eps = 1.0)\n  print_success(sprintf(\"Computed priors for %s actions\", \n                       bold(as.character(nrow(action_priors$global)))))\n  \n  cat(\"\\n\", cyan(\"ğŸ”„ Generating advanced baseline predictions...\"), \"\\n\", sep = \"\")\n  train_submission <- create_advanced_baseline(\n    train_subset, \n    traintest = \"train\",\n    action_priors = action_priors,\n    use_windows = TRUE,\n    min_segment_len = 10,\n    gap_close = 5,\n    rare_action_cap = 0.02\n  )\n  print_success(sprintf(\"Generated %s predictions\", bold(as.character(nrow(train_submission)))))\n  \n  cat(\"\\n\", cyan(\"ğŸ§® Calculating F1 score...\"), \"\\n\", sep = \"\")\n  cat(yellow(\"â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\\n\"))\n  \n  start_time <- Sys.time()\n  validation_score <- calculate_f1_score(solution_df, train_submission, beta = 1)\n  end_time <- Sys.time()\n  time_taken <- as.numeric(difftime(end_time, start_time, units = \"secs\"))\n  \n  cat(sprintf(\"â±ï¸ Time taken: %s seconds\\n\\n\", bold(sprintf(\"%.2f\", time_taken))))\n  \n  # Display score prominently with color\n  cat(green(\"â•”â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•—\\n\"))\n  cat(green(\"â•‘\"), bold(cyan(sprintf(\" VALIDATION SCORE: %.15f \", validation_score))), green(\"â•‘\\n\"))\n  cat(green(\"â•šâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•\\n\"))\n  \n  cat(\"\\n\", yellow(\"Target score: >0.11\"), \" (beating Python baseline)\\n\", sep = \"\")\n  \n  cat(\"\\n\", magenta(\"ğŸ“Š Validation Statistics:\"), \"\\n\", sep = \"\")\n  cat(yellow(\"â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\\n\"))\n  cat(sprintf(\"  %s Training videos used: %s\\n\", green(\"â€¢\"), bold(n_distinct(train_subset$video_id))))\n  cat(sprintf(\"  %s Total annotations: %s\\n\", green(\"â€¢\"), bold(nrow(solution_df))))\n  cat(sprintf(\"  %s Total predictions: %s\\n\", green(\"â€¢\"), bold(nrow(train_submission))))\n  cat(sprintf(\"  %s Unique behaviors: %s\\n\", green(\"â€¢\"), bold(n_distinct(solution_df$action))))\n  \n  # Save validation results if EDA mode is on\n  if(SAVE_EDA_FILES) {\n    validation_results <- data.frame(\n      metric = c(\"F1 Score\", \"Time (seconds)\", \"Training Videos\", \n                 \"Total Annotations\", \"Total Predictions\"),\n      value = c(validation_score, time_taken, n_distinct(train_subset$video_id),\n                nrow(solution_df), nrow(train_submission))\n    )\n    write_csv(validation_results, \"EDA/MABe_validation_results.csv\")\n    cat(\"\\n\")\n    print_success(\"Validation results saved to: EDA/MABe_validation_results.csv\")\n  }\n}\n\n# ============================================\n# 10. CREATE TEST SUBMISSION\n# ============================================\n\nprint_header(\"STEP 10: CREATING TEST SUBMISSION\", 2)\n\nif(!is.null(test)) {\n  # Check if action_priors exists from validation\n  if(!exists(\"action_priors\") || is.null(action_priors)) {\n    cat(yellow(\"âš ï¸ Action priors not found. Computing from available data...\\n\"))\n    \n    # Try to compute from solution_df if it exists\n    if(exists(\"solution_df\") && nrow(solution_df) > 0) {\n      action_priors <- compute_action_priors(solution_df, eps = 1.0)\n      print_success(\"Computed action priors from training data\")\n    } else {\n      # Create empty priors structure (will use uniform weights)\n      cat(yellow(\"âš ï¸ No training data available. Using uniform priors.\\n\"))\n      action_priors <- list(\n        global = data.frame(),\n        by_lab = data.frame(),\n        global_durations = data.frame(),\n        lab_durations = data.frame()\n      )\n    }\n  }\n  \n  cat(cyan(\"ğŸš€ Using advanced baseline with proximity windows...\\n\\n\"))\n  \n  test_submission <- create_advanced_baseline(\n    test, \n    traintest = \"test\",\n    action_priors = action_priors,\n    use_windows = TRUE,\n    min_segment_len = 10,\n    gap_close = 5,\n    rare_action_cap = 0.02\n  )\n  \n  if(nrow(test_submission) > 0) {\n    # Add row_id column for submission\n    test_submission <- test_submission |>\n      mutate(row_id = row_number() - 1) |>  # 0-indexed row IDs\n      select(row_id, everything())  # Put row_id first\n    \n    # Write without quotes and with proper formatting\n    write.csv(test_submission, \"submission.csv\", row.names = FALSE, quote = FALSE)\n    \n    print_success(sprintf(\"Created submission.csv with %s predictions\", bold(as.character(nrow(test_submission)))))\n    cat(cyan(\"ğŸ“ˆ Strategy:\"), \"Using proximity windows and action priors\\n\")\n    \n    cat(\"\\n\", magenta(\"ğŸ“‹ First 10 rows of submission:\"), \"\\n\", sep = \"\")\n    print(head(test_submission, 10))\n    \n    # Verify the format\n    cat(\"\\n\", cyan(\"âœ”ï¸ Verifying submission format:\"), \"\\n\", sep = \"\")\n    cat(sprintf(\"  - Columns: %s\\n\", yellow(paste(names(test_submission), collapse = \", \"))))\n    cat(sprintf(\"  - Number of rows: %s\\n\", bold(nrow(test_submission))))\n    cat(sprintf(\"  - Sample agent_id: %s (no quotes)\\n\", green(test_submission$agent_id[1])))\n    cat(sprintf(\"  - Sample action: %s (no quotes)\\n\", green(test_submission$action[1])))\n    \n    if(SAVE_EDA_FILES) {\n      write.csv(test_submission, \"EDA/MABe_baseline_submission.csv\", \n                row.names = FALSE, quote = FALSE)\n      cat(\"\\n\")\n      print_success(\"Copy saved to: EDA/MABe_baseline_submission.csv\")\n    }\n  } else {\n    print_warning(\"No predictions generated for test data\")\n  }\n} else {\n  print_warning(\"No test data available - cannot create submission\")\n}\n\n# ============================================\n# 11. KEY INSIGHTS\n# ============================================\n\nif(VERBOSE_OUTPUT) {\n  print_header(\"KEY INSIGHTS\", 2)\n  \n  insights <- c(\n    \"Some labs (MABe22_keypoints, MABe22_movies) appear to have no annotations\",\n    \"The dataset contains multiple experimental setups from different labs\",\n    \"Behaviors show significant variation in frequency and duration\",\n    \"Social behaviors can be distinguished using agent and target IDs\",\n    \"Tracking data includes detailed body part positions for each mouse\",\n    \"The 'rear' behavior is the most common, accounting for ~26% of annotations\",\n    \"Most behaviors are social interactions (different agent and target IDs)\",\n    \"Behavior durations vary widely, with 'mount' having the longest average duration\",\n    \"The improved baseline distributes predictions across all actions for better F1 scores\"\n  )\n  \n  for(i in seq_along(insights)) {\n    cat(sprintf(\"%s %d. %s\\n\", \n                ifelse(i <= 3, cyan(\"ğŸ’¡\"), green(\"â€¢\")),\n                i, insights[i]))\n  }\n  \n  # Save insights to file only if EDA files are enabled\n  if(SAVE_EDA_FILES) {\n    writeLines(insights, \"EDA/MABe_key_insights.txt\")\n    cat(\"\\n\")\n    print_success(\"Key insights saved to: EDA/MABe_key_insights.txt\")\n  }\n}\n\n# ============================================\n# 12. CREATE SUMMARY REPORT\n# ============================================\n\nif(SAVE_EDA_FILES) {\n  print_header(\"CREATING SUMMARY REPORT\", 2)\n  \n  report_lines <- c(\n    \"MABe CHALLENGE - EDA SUMMARY REPORT\",\n    \"====================================\",\n    paste(\"Generated:\", Sys.Date()),\n    \"\",\n    \"DATASET OVERVIEW\",\n    \"----------------\",\n    paste(\"- Total training videos:\", nrow(train)),\n    paste(\"- Total test videos:\", ifelse(!is.null(test), nrow(test), 0)),\n    paste(\"- Number of labs:\", length(unique_labs)),\n    paste(\"- Labs with annotations:\", length(labs_with_annotations)),\n    \"\",\n    \"BEHAVIOR ANALYSIS\",\n    \"-----------------\",\n    paste(\"- Total annotations analyzed:\", nrow(annotations_df)),\n    paste(\"- Unique behaviors:\", ifelse(nrow(annotations_df) > 0, n_distinct(annotations_df$action), 0)),\n    paste(\"- Most frequent behavior:\", ifelse(nrow(annotations_df) > 0, behavior_counts$action[1], \"N/A\")),\n    \"\",\n    \"VALIDATION RESULTS\",\n    \"------------------\",\n    paste(\"- F1 Score:\", ifelse(exists(\"validation_score\"), sprintf(\"%.5f\", validation_score), \"N/A\")),\n    paste(\"- Expected score: ~0.066\"),\n    \"\",\n    \"FILES GENERATED\",\n    \"---------------\",\n    \"Visualizations:\",\n    \"- EDA/MABe_behavior_frequency.png\",\n    \"- EDA/MABe_behavior_duration.png\",\n    \"- EDA/MABe_lab_distribution.png\",\n    \"- EDA/MABe_social_behaviors.png\",\n    \"- EDA/MABe_behavior_percentage.png\",\n    \"- EDA/MABe_duration_vs_frequency.png\",\n    \"\",\n    \"Data Files:\",\n    \"- EDA/MABe_summary_statistics.csv\",\n    \"- EDA/MABe_validation_results.csv\",\n    \"- EDA/MABe